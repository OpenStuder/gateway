{
  "name": "SQLite",
  "version": "1.0",
  "author": "michael.clausen@hevs.ch",
  "parameters": [
    {
      "name": "file",
      "type": "QString",
      "required": false,
      "default": "/var/lib/sigateway/storage.sqlite"
    },
    {
      "name": "cleanupInterval",
      "type": "int",
      "required": false,
      "default": 3600
    },
    {
      "name": "maxStorageDays",
      "type": "int",
      "required": false,
      "default": 730
    }
  ]
}
